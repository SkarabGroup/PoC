# MongoDB
MONGODB_URI=mongodb://localhost:27017/
MONGODB_DB_NAME=agenti_db

# JWT
JWT_SECRET=your-super-secret-jwt-key-change-in-production
JWT_EXPIRATION=15m
JWT_REFRESH_SECRET=your-refresh-secret-key
JWT_REFRESH_EXPIRATION=7d

# Redis (Bull Queue)
REDIS_HOST=localhost
REDIS_PORT=6379

# Python Agents
PYTHON_PATH=./venv/bin/python
ORCHESTRATOR_PATH=./src/agents/orchestrator.py
TEMP_PATH=./tempCloned

# AWS Bedrock
AWS_ACCESS_KEY_ID=your-aws-access-key-id
AWS_SECRET_ACCESS_KEY=your-aws-secret-access-key
AWS_SESSION_TOKEN=your-aws-session-token
AWS_REGION=eu-central-1
ORCHESTRATOR_BEDROCK_MODEL_ID=arn:aws:bedrock:eu-central-1::inference-profile/eu.amazon.nova-lite-v1:0
MAX_AGENT_ITERATIONS=20

# Frontend URL (CORS)
FRONTEND_URL=http://localhost:5173

# Server
PORT=3000
NODE_ENV=development

# Analysis Mode
USE_MOCK_ANALYSIS=true
